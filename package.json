{
  "name": "mahad-autopay",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "cleanup": "npm run format && npm run lint -- --fix && npm run type-check",
    "cleanup-stripe": "tsx scripts/cleanup-stripe.ts",
    "see-active": "ts-node scripts/see-active.ts",
    "tunnel": "npx localtunnel --port 3000 --subdomain mahad-dev",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "webhook": "stripe listen --forward-to localhost:3000/api/webhook --events customer.subscription.created,customer.subscription.updated,customer.subscription.deleted,invoice.payment_succeeded,invoice.payment_failed,payment_intent.payment_failed",
    "dev:webhook": "concurrently \"npm run dev\" \"npm run webhook\"",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
